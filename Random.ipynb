    "import random\n",
    "number = random.randint(1,100)\n",
    "guess = 0\n",
    "while guess != number:\n",
    "  guess = int(input(\"Enter your Guess: \"))\n",
    "\n",
    "  if (guess < number):\n",
    "    print(\"NOT- GUESS HIGHER!\")\n",
    "  elif (guess > number):\n",
    "    print(\"NOT- GUESS LOWER!\")\n",
    "  else:\n",
    "    print(\"You won!\")"
